#include <stdio.h>

int main() {
    int arr[] = {0, 1, 0, 3, 12, 0, 5};
    int size = sizeof(arr) / sizeof(arr[0]);
    int pos = 0;  // Index to place next non-zero element

    // Step 1: Move non-zero elements to the front
    for (int i = 0; i < size; i++) {
        if (arr[i] != 0) {
            arr[pos++] = arr[i];
        }
    }

    // Step 2: Fill the rest with zeros
    while (pos < size) {
        arr[pos++] = 0;
    }

    // Print the result
    printf("Array after moving all zeros to the end:\n");
    for (int i = 0; i < size; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
