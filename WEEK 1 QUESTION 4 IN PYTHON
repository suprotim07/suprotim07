arr = [0, 1, 0, 3, 12, 0, 5]

pos = 0  # Position to place the next non-zero

# Move all non-zero elements to the front
for num in arr:
    if num != 0:
        arr[pos] = num
        pos += 1

# Fill remaining positions with zeros
while pos < len(arr):
    arr[pos] = 0
    pos += 1

print("After moving zeros to the end:", arr)
